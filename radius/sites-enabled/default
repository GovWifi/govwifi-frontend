server default {
listen {
        type = auth


        ipaddr = *

        port = 0



        limit {
              max_connections = 64


              lifetime = 0

              idle_timeout = 30
        }
}

listen {
        ipaddr = *
        port = 0
        type = acct

        limit {

        }
}

authorize {
	eap {
		ok = return
	}
}
authenticate {
	eap
}
preacct {
}
accounting {
}
session {
}
post-auth {
  # Capture TLS version for all TLS-based EAP methods
  if (&EAP-Type =~ /^(TLS|PEAP|TTLS)$/) {
    tlsver
  }

 if (EAP-Type == TLS)  {
   redundant {
     rest
     ok
   }
 }
}
pre-proxy {
}
post-proxy {
	eap
}
}
